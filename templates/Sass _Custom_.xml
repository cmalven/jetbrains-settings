<templateSet group="Sass (Custom)">
  <template name="mq" value="@include mq($BREAKPOINT$) {&#10;  $END$&#10;}" description="sass-mq query" toReformat="false" toShortenFQNames="true">
    <variable name="BREAKPOINT" expression="enum(&quot;medium&quot;, &quot;large&quot;, &quot;xlarge&quot;, &quot;full&quot;, &quot;max&quot;) " defaultValue="medium" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="mqu" value="@include mq($until: $BREAKPOINT$) {&#10;  $END$&#10;}" description="sass-mq until query" toReformat="false" toShortenFQNames="true">
    <variable name="BREAKPOINT" expression="enum(&quot;medium&quot;, &quot;large&quot;, &quot;xlarge&quot;, &quot;full&quot;, &quot;max&quot;) " defaultValue="medium" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="c2" value="// ---------------------------------------------------------------&#10;// $CONTENT$&#10;// ---------------------------------------------------------------&#10;&#10;$END$&#10;" description="Comment (Level 2)" toReformat="false" toShortenFQNames="true">
    <variable name="CONTENT" expression="" defaultValue="Comment Content" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fls" value="@include fluid-size($SIZE$$PROPS$);" description="Fluid Size" toReformat="false" toShortenFQNames="true">
    <variable name="SIZE" expression="&quot;m&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="PROPS" expression="&quot;, margin-top&quot;" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="fluid" value="@include fluid(&#10;  $PROPS$,&#10;  $START$,&#10;  $STOP$,&#10;  $MIN$px,&#10;  $MAX$px&#10;);" description="Fluid" toReformat="false" toShortenFQNames="true">
    <variable name="PROPS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="START" expression="enum(&quot;medium&quot;, &quot;large&quot;, &quot;xlarge&quot;, &quot;full&quot;, &quot;max&quot;) " defaultValue="default" alwaysStopAt="true" />
    <variable name="STOP" expression="enum(&quot;medium&quot;, &quot;large&quot;, &quot;xlarge&quot;, &quot;full&quot;, &quot;max&quot;) " defaultValue="max" alwaysStopAt="true" />
    <variable name="MIN" expression="" defaultValue="50" alwaysStopAt="true" />
    <variable name="MAX" expression="" defaultValue="100" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="anim" value="animation-name: none;&#10;animation-duration: 0s;&#10;animation-timing-function: ease;&#10;animation-delay: 0s;&#10;animation-iteration-count: infinite;" description="CSS animation" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
</templateSet>